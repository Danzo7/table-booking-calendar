{
  "name": "table-booking-calendar",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Table booking calendar",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/table-booking-calendar.es.js",
      "require": "./dist/table-booking-calendar.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/table-booking-calendar.umd.js",
  "module": "./dist/table-booking-calendar.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"yarn build\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint --ext .ts,.tsx src/",
    "eslint-fix": "eslint --fix  --ext .ts,.tsx src/",
    "check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.7",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-tooltip": "^5.7.5",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^3.1.0",
    "add": "^2.0.6",
    "babel-loader": "^8.3.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  }
}
